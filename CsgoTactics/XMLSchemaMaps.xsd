                    <xs:element name="MapType" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="MapDescription" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="MapCoord" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Xcoord" type="xs:int" minOccurs="1" maxOccurs="1"/>
                          <xs:element name="Ycoord" type="xs:int" minOccurs="1" maxOccurs="1"/>
